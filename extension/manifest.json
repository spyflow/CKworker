{
    "manifest_version": 3,
    "name": "CKworker",
    "version": "1.3",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "/popup/html/index.html",
      "default_icon": {
        "16": "/icons/icon.png",
        "48": "/icons/icon.png",
        "128": "/icons/icon.png"
      }
    },
    "background": {
      "service_worker": "/background/js/background.js"
    },
    "icons": {
      "16": "/icons/icon.png",
      "48": "/icons/icon.png",
      "128": "/icons/icon.png"
    }
  }
  